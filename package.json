{
  "name": "@heeler/ui",
  "version": "1.0.0",
  "description": "Open source component library for React applications",
  "author": "Happy Heeler Technologies",
  "maintainers": [
    "Brian Lindquist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Happy-Heeler-Technologies/heeler-ui.git"
  },
  "homepage": "https://github.com/Happy-Heeler-Technologies/heeler-ui#readme",
  "bugs": {
    "url": "https://github.com/Happy-Heeler-Technologies/heeler-ui/issues"
  },
  "keywords": [
    "react",
    "react-components",
    "ui",
    "component-library",
    "design-system",
    "typescript",
    "tailwind",
    "tailwindcss",
    "rainbow",
    "accessibility"
  ],
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs.js"
      }
    },
    "./styles.css": "./dist/index.css"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "dist/index.css"
  ],
  "dependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@storybook/addon-a11y": "^10.1.0",
    "@storybook/addon-docs": "^10.1.0",
    "@storybook/addon-vitest": "^10.1.0",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react-vite": "^10.1.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/browser-playwright": "^4.0.14",
    "@vitest/coverage-v8": "^4.0.14",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-storybook": "^10.1.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "storybook": "^10.1.0",
    "tailwindcss": "^4.1.17",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.14"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
